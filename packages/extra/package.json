{
  "name": "@pumped-fn/extra",
  "version": "0.0.0",
  "description": "Extra utilities for pumped-fn",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "typecheck:test": "vitest typecheck"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@pumped-fn/core": "^0.5.0"
  },
  "engines": {
    "node": ">=16"
  },
  "license": "MIT",
  "dependencies": {
    "hono": "^4.7.2",
    "zod": "^3.24.2"
  }
}
